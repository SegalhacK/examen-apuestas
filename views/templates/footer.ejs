<script>


    $(document).ready(function () {


        $('#modalAdd').on('shown.bs.modal', function () {
            $('#myInput').trigger('focus')
        })

        moment.locale("es");

        toastr.options = {
            "closeButton": true,
            "progressBar": true,
        }

        bootbox.addLocale("pancho", {
